{
	"Highlighter": {
		"Syntax": [
			"while",
			"for",
			"in",
			"do",
			"function",
			"local",
			"if",
			"not",
			"and",
			"or",
			"then",
			"else",
			"elseif",
			"end",
			"return",
			"goto",
			"{", "}", "[", "]", ",", "#",
			"=", "+", "-", "*", "/", "^"
		],
		"String": [
			"\"", 
			"'"
		],
		"Comment": [
			"--",
		],
		"MultilineComment": {
			"Start": [
				"--[[",
			],
			"End": [
				"]]"
			]
		},
		"Bool": [
			"true", 
			"false"
		],
		"WordPattern": "\\w+|--[[]+|]]|--|\\\\\\W|\\W"
	},
	"RunCommands": {
		"Lua": "%FilePath%",
		"Love2D": "%FolderPath%",
	}
}
